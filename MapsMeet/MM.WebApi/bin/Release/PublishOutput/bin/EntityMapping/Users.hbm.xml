<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="MM.ApplicationCore" namespace="MM.ApplicationCore.Entities">
  <class name="Users">
    <id name="UserId">
      <generator class="native" />
    </id>
    <property name="FirstName" not-null="true" />
    <property name="LastName" />
    <property name="Email" not-null="true" />
    <property name="Gender" not-null="true" />
    <property name="DOB" not-null="true" />
    <property name="MobileNo" />
    <property name="Token" />
    <property name="LoginType" not-null="true" />
    <property name="CreatedOn" not-null="true" />
    <property name="UpdatedOn" />
  </class>
</hibernate-mapping>