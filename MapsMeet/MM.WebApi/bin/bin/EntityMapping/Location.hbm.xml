<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="MM.ApplicationCore" namespace="MM.ApplicationCore.Entities">
  <class name="Location">
    <id name="Id">
      <generator class="native" />
    </id>
    <property name="Longitude" not-null="true" />
    <property name="Latitude" not-null="true" />
    <property name="CreatedOn" not-null="true" />
    <property name="UpdatedOn" />
    
    <one-to-one name="Users" class="Users"/>

  </class>
</hibernate-mapping>